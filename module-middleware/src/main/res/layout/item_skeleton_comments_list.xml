<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:paddingTop="@dimen/dp_16"
	android:paddingBottom="@dimen/dp_16">

	<com.jmbon.widget.SuperView
		android:id="@+id/iv_header"
		android:layout_width="@dimen/dp_34"
		android:layout_height="@dimen/dp_34"
		android:layout_marginStart="@dimen/dp_20"

		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:vCornersRadius="@dimen/dp_20"
		app:vSelectorNormalColor="@color/colorF5F5F5"
		app:vSelectorPressedColor="@color/colorF5F5F5"
		app:vUseSelector="true" />

	<com.jmbon.widget.SuperView
		android:id="@+id/view_1"
		android:layout_width="@dimen/dp_66"
		android:layout_height="@dimen/dp_20"
		android:layout_marginStart="@dimen/dp_10"
		app:layout_constraintStart_toEndOf="@id/iv_header"
		app:layout_constraintTop_toTopOf="@id/iv_header"
		app:vCornersRadius="@dimen/dp_4"
		app:vSelectorNormalColor="@color/colorF5F5F5"
		app:vSelectorPressedColor="@color/colorF5F5F5"
		app:vUseSelector="true" />

	<com.jmbon.widget.SuperView
		android:id="@+id/view_4"
		android:layout_width="@dimen/dp_60"
		android:layout_height="@dimen/dp_14"
		android:layout_marginTop="@dimen/dp_14"
		app:layout_constraintStart_toStartOf="@id/view_3"
		app:layout_constraintTop_toBottomOf="@id/view_3"
		app:vCornersRadius="@dimen/dp_3"
		app:vSelectorNormalColor="@color/colorF5F5F5"
		app:vSelectorPressedColor="@color/colorF5F5F5"
		app:vUseSelector="true" />

	<com.jmbon.widget.SuperView
		android:id="@+id/view_2"
		android:layout_width="240dp"
		android:layout_height="@dimen/dp_16"
		android:layout_marginTop="@dimen/dp_8"
		app:layout_constraintStart_toStartOf="@id/view_1"
		app:layout_constraintTop_toBottomOf="@id/view_1"
		app:vCornersRadius="@dimen/dp_3"
		app:vSelectorNormalColor="@color/colorF5F5F5"
		app:vSelectorPressedColor="@color/colorF5F5F5"
		app:vUseSelector="true" />


	<com.jmbon.widget.SuperView
		android:id="@+id/view_3"
		android:layout_width="240dp"
		android:layout_height="@dimen/dp_16"
		android:layout_marginTop="@dimen/dp_4"
		app:layout_constraintStart_toStartOf="@id/view_1"
		app:layout_constraintTop_toBottomOf="@id/view_2"
		app:vCornersRadius="@dimen/dp_3"
		app:vSelectorNormalColor="@color/colorF5F5F5"
		app:vSelectorPressedColor="@color/colorF5F5F5"
		app:vUseSelector="true" />


	<com.jmbon.widget.SuperView
		android:id="@+id/view_5"
		android:layout_width="@dimen/dp_24"
		android:layout_height="@dimen/dp_24"
		android:layout_marginEnd="@dimen/dp_20"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="@id/iv_header"
		app:vCornersRadius="@dimen/dp_5"
		app:vSelectorNormalColor="@color/colorF5F5F5"
		app:vSelectorPressedColor="@color/colorF5F5F5"
		app:vUseSelector="true" />
</androidx.constraintlayout.widget.ConstraintLayout>