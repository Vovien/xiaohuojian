<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape android:shape="rectangle">
			<corners android:radius="25dp" />
			<solid android:color="#1AFF5050" />
			<size
				android:width="50dp"
				android:height="50dp" />
			<padding
				android:bottom="12dp"
				android:left="12dp"
				android:right="12dp"
				android:top="12dp" />
		</shape>
	</item>
	<item>
		<bitmap
			android:gravity="center"
			android:src="@drawable/icon_blacklist" />
	</item>
</layer-list>